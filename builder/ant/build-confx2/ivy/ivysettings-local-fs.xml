<ivysettings>
    <caches>
        <!--<cache name="main" basedir="${ivy.settings.dir}/ivycache" />-->
        <cache name="nocache" useOrigin="true" />
    </caches>
<resolvers>
    <filesystem name="local-fs" cache="nocache">
        <artifact pattern="${builder.dir}/repo/[artifact](-[revision]).[ext]" />
        <artifact pattern="${builder.dir}/repo/[artifact].[ext]" />
    </filesystem>
</resolvers>
</ivysettings>