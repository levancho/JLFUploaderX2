<ivysettings>
	<settings defaultResolver="default"/>

	<caches default="dev-cache" resolutionCacheDir="${user.home}/.ivy/${build.env}/resocache"
			repositoryCacheDir="${user.home}/.ivy/${build.env}/cache" >
		<cache name="snap-cache" basedir="${user.home}/.ivy/snap-cache" />
		<cache name="dev-cache" basedir="${user.home}/.ivy/dev-cache" />
	</caches>

	<!--<include url="${ivy.settings.dir}/myivysettings-public.xml"/>-->
	<!--<include url="${ivy.default.settings.dir}/ivysettings-public.xml"/>-->
	<include url="ivysettings-publicx.xml"/>
	<include url="${build.env}/ivysettings-sharedx.xml"/>
	<include url="${ivy.default.settings.dir}/ivysettings-local.xml"/>
	<include url="ivysettings-local-fs.xml"/>
	<include url="${build.env}/ivysettings-main-chain.xml"/>
	<include url="ivysettings-default-chain.xml"/>

	<statuses default="prod">
		<status name="prod" integration="false"/>
		<status name="release" integration="false"/>
		<status name="integ" integration="true"/>
		<status name="integration" integration="true"/>
	</statuses>

